@model IEnumerable<AdoRelation.Models.Department>


<button class="btn btn-primary mb-3 text-white"><a class="text-white" href="@Url.Action("Create")">Add Department</a> </button>
<table class="border table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Department Name</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.DepartmentName</td>
            </tr>
        }
        
       
    </tbody>
</table>
